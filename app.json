{
  "expo": {
    "name": "MyKonbini",
    "slug": "my-konbini",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MyKonbiniIcon.png",
    "scheme": "mykonbini",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ambatron.MyKonbini"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MyKonbiniIcon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ambatron.MyKonbini",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/MyKonbiniIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9e43f791-c546-4414-a64b-342ce8f35948"
      }
    }
  }
}
